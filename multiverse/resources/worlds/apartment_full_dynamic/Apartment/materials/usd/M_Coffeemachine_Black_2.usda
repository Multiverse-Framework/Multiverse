#usda 1.0
(
    defaultPrim = "M_Coffeemachine_Black_2"
    upAxis = "Z"
)

def Material "M_Coffeemachine_Black_2"
{
    token outputs:surface.connect = </M_Coffeemachine_Black_2/PBRShader.outputs:surface>

    def Shader "PBRShader"
    {
        uniform token info:id = "UsdPreviewSurface"
        color3f inputs:diffuseColor = (0.016807375, 0.016807375, 0.016807375)
        float inputs:opacity = 1
        int inputs:useSpecularWorkflow = 1
        token outputs:surface
    }
}

