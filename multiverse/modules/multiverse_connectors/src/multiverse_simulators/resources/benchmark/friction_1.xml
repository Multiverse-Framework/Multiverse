<mujoco model="friction_1">
  <option gravity="1.0 0 -1.0" timestep="0.001" tolerance="0" >
    <flag multiccd="enable" nativeccd="enable"/>
  </option>

  <visual>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="4096"/>
    <map stiffness="700" shadowscale="0.5" fogstart="1" fogend="15" zfar="40" haze="1"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
    <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2"
      width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>

    <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="10 10" texuniform="true"/>
  </asset>

  <worldbody>
    <light diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 10.0" dir="0 0 -1" castshadow="false"/>
    <light directional="true" diffuse=".8 .8 .8" specular="0.2 0.2 0.2" pos="0 0 10" dir="0 0 -1"/>

    <geom name="ground" type="plane" size="2 1 1" pos="1.5 -0.8 0" quat="1 0 0 0" material="matplane" condim="1" friction="0 0 0"/>
    <site name="line_2_x" type="cylinder" pos="1.0 -0.8 0.0" size="0.001 1" rgba="1 0 0 1" quat="1 1 0 0"/>
    <site name="line_0.5_x" type="cylinder" pos="0.5 -0.8 0.0" size="0.001 1" rgba="1 1 0 1" quat="1 1 0 0"/>
    <site name="line_0.25_x" type="cylinder" pos="0.25 -0.8 0.0" size="0.001 1" rgba="0 1 0 1" quat="1 1 0 0"/>
  </worldbody>

  <worldbody>
    <body name="box_1" pos="0.0 0.0 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E9 1E9 1E9" />
      <geom name="box_1_geom" type="box" size="0.05 0.05 0.05" rgba="0.9 0.9 0.9 1.0" condim="3" friction="0 0 0"/>
    </body>

    <body name="box_2" pos="0.0 -0.2 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E9 1E9 1E9" />
      <geom name="box_2_geom" type="box" size="0.05 0.05 0.05" rgba="0.9 0.9 0.9 1.0" condim="3" friction="0.5 0 0"/>
    </body>

    <body name="box_3" pos="0.0 -0.4 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E9 1E9 1E9" />
      <geom name="box_3_geom" type="box" size="0.05 0.05 0.05" rgba="0.9 0.9 0.9 1.0" condim="3" friction="1.0 0 0"/>
    </body>

    <body name="sphere_1" pos="0.0 -0.6 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
      <geom name="sphere_1_geom" type="sphere" size="0.05" rgba="0.9 0.9 0.9 1.0" condim="6" friction="1.0 0 0"/>
    </body>

    <body name="sphere_2" pos="0.0 -0.8 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
      <geom name="sphere_2_geom" type="sphere" size="0.05" rgba="0.9 0.9 0.9 1.0" condim="6" friction="1.0 0.0 0.05"/>
    </body>

    <body name="sphere_3" pos="0.0 -1.0 0.05">
      <freejoint />
      <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
      <geom name="sphere_3_geom" type="sphere" size="0.05" rgba="0.9 0.9 0.9 1.0" condim="6" friction="1.0 0.0 0.1"/>
    </body>

    <body name="cylinder_1" pos="0.0 -1.2 0.05">
      <joint name="cylinder_1_joint_1" type="slide" axis="1 0 0" />
      <joint name="cylinder_1_joint_2" type="slide" axis="0 0 1" />
      <inertial pos="0 0 0" mass="1E-9" diaginertia="1E-9 1E-9 1E-9" />

      <body name="cylinder_1_main" >
        <joint name="cylinder_1_main_joint" type="hinge" axis="0 1 0" />
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_1_main_geom" euler="90.0 0 0" type="cylinder" size="0.05 0.015" rgba="0.9 0.9 0.9 1.0" condim="4" friction="1 0 0" />
      </body>

      <body name="cylinder_1_pressure_1" pos="0.0 0.03 0.0">
        <joint name="cylinder_1_pressure_1_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="-1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_1_pressure_1_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0 0" priority="1"/>
      </body>

      <body name="cylinder_1_pressure_2" pos="0.0 -0.03 0.0">
        <joint name="cylinder_1_pressure_2_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_1_pressure_2_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0 0" priority="1"/>
      </body>
    </body>

    <body name="cylinder_2" pos="0.0 -1.4 0.05">
      <joint name="cylinder_2_joint_1" type="slide" axis="1 0 0" />
      <joint name="cylinder_2_joint_2" type="slide" axis="0 0 1" />
      <inertial pos="0 0 0" mass="1E-9" diaginertia="1E-9 1E-9 1E-9" />

      <body name="cylinder_2_main" >
        <joint name="cylinder_2_main_joint" type="hinge" axis="0 1 0" />
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_2_main_geom" euler="90.0 0 0" type="cylinder" size="0.05 0.015" rgba="0.9 0.9 0.9 1.0" condim="4" friction="1 0 0" />
      </body>

      <body name="cylinder_2_pressure_1" pos="0.0 0.03 0.0">
        <joint name="cylinder_2_pressure_1_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="-1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_2_pressure_1_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0.05 0" priority="1"/>
      </body>

      <body name="cylinder_2_pressure_2" pos="0.0 -0.03 0.0">
        <joint name="cylinder_2_pressure_2_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_2_pressure_2_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0.05 0" priority="1"/>
      </body>
    </body>

    <body name="cylinder_3" pos="0.0 -1.6 0.05">
      <joint name="cylinder_3_joint_1" type="slide" axis="1 0 0" />
      <joint name="cylinder_3_joint_2" type="slide" axis="0 0 1" />
      <inertial pos="0 0 0" mass="1E-9" diaginertia="1E-9 1E-9 1E-9" />

      <body name="cylinder_3_main" >
        <joint name="cylinder_3_main_joint" type="hinge" axis="0 1 0" />
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_3_main_geom" euler="90.0 0 0" type="cylinder" size="0.05 0.015" rgba="0.9 0.9 0.9 1.0" condim="4" friction="1 0 0" />
      </body>

      <body name="cylinder_3_pressure_1" pos="0.0 0.03 0.0">
        <joint name="cylinder_3_pressure_1_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="-1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_3_pressure_1_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0.1 0" priority="1"/>
      </body>

      <body name="cylinder_3_pressure_2" pos="0.0 -0.03 0.0">
        <joint name="cylinder_3_pressure_2_joint_2" type="slide" axis="0 1 0" stiffness="1" springref="1"/>
        <inertial pos="0 0 0" mass="1" diaginertia="1E-9 1E-9 1E-9" />
        <geom name="cylinder_3_pressure_2_geom" euler="90.0 0 0" type="cylinder" size="0.025 0.015" rgba="0.9 0.0 0.0 0.2" condim="4" friction="0 0.1 0" priority="1"/>
      </body>
    </body>
  </worldbody>

  <sensor>
    <framepos name="box_1_pos" objtype="body" objname="box_1" />
    <framepos name="box_2_pos" objtype="body" objname="box_2" />
    <framepos name="box_3_pos" objtype="body" objname="box_3" />
    <framepos name="sphere_1_pos" objtype="body" objname="sphere_1" />
    <framepos name="sphere_2_pos" objtype="body" objname="sphere_2" />
    <framepos name="sphere_3_pos" objtype="body" objname="sphere_3" />
    <framepos name="cylinder_1_pos" objtype="body" objname="cylinder_1" />
    <framepos name="cylinder_2_pos" objtype="body" objname="cylinder_2" />
    <framepos name="cylinder_3_pos" objtype="body" objname="cylinder_3" />
  </sensor>
</mujoco>